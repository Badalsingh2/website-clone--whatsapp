{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "installCommand": "npm install",
  "functions": {
    "*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/.next/server/pages/$1"
    }
  ]
}
